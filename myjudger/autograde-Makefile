python:
	@tar xf autograde.tar
	@cp Solution autograde/Solution.py
	@chmod o+r   autograde/Solution.py
	@cd autograde;python3 Judge.py python
java:
	@tar xf autograde.tar
	@cp Solution autograde/Solution.java
	@chmod o+r   autograde/Solution.java
	@cd autograde;python3 Judge.py java

clean:
	rm -rf *~ autograde
